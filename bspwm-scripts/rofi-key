#! /bin/sh
# custom bspwm keybindings cheatsheet
dunstify -u normal \
"BSPWM keybindings:" \
"S + p          rofi\n\
S + Return      Term\n\
S + f           Files\n\
S + b		    Firefox\n\
S + 1-5	        go to ws\n\
S + Sh + 1-5	mv win to ws\n\
S + Sh + ,    mv win\n\
S + q           close win\n\
S + x           exit\n\
S + Sh + r	    reload bspwm\n\
"
